{
  "name": "gistwiz",
  "version": "0.4.0",
  "type": "module",
  "module": "./src/cli.ts",
  "bin": {
    "gistwiz": "./src/cli.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "@octokit/plugin-retry": "latest",
    "@octokit/plugin-throttling": "latest",
    "@octokit/rest": "latest",
    "@types/ms": "^0.7.34",
    "bullmq": "latest",
    "commander": "latest",
    "ioredis": "latest",
    "ms": "^2.1.3"
  },
  "scripts": {
    "gistwiz": "bun run src/cli.ts",
