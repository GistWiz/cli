{
  "name": "gistwiz",
  "version": "0.7.0",
  "type": "module",
  "module": "./src/cli.ts",
  "bin": {
    "gistwiz": "./src/cli.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "@octokit/plugin-retry": "latest",
    "@octokit/plugin-throttling": "latest",
    "@octokit/rest": "latest",
    "@types/ms": "latest",
    "bullmq": "latest",
    "commander": "latest",
    "ioredis": "latest",
    "ms": "latest"
  },
  "scripts": {
    "gistwiz": "bun run src/cli.ts",
    "monitor": "./src/bin/redis-file-monitor.sh",
    "pro": "export $(grep -v '^#' .env | xargs) && ./src/bin/pro digitalocean --repo GistWiz/cli --name 'GistWizQuickSearch' --tags 'GistWiz,QuickSearch'"
  },
  "trustedDependencies": [
    "core-js"
  ]
}